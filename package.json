{
  "name": "onyx-drift-app-final",
  "version": "1.0.0",
  "description": "Fullstack application for OnyxDrift.",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "build": "npm install && cd client && npm install && npm run build",
    "dev": "concurrently \"npm run server\" \"cd client && npm run dev\""
  },
  "engines": {
    "node": "22.12.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.12.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "socket.io": "4.8.1",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.7",
    "vite": "^5.0.0"
  }
}
