{
  "name": "onyx-drift-app-final",
  "version": "1.0.0",
  "description": "Fullstack application for OnyxDrift.",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "build": "cd client && npm install --legacy-peer-deps && npm run build",
    "dev": "concurrently \"npm run server\" \"cd client && npm run dev\""
  },
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-oauth2-jwt-bearer": "^1.7.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.21.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.7"
  }
}